#include <iostream>
#include <vector>
#include <ctime>
#include <cstdlib>
using namespace std;

void insertionSort(vector<int> &arr) {
    int n = arr.size();
    for (int i = 1; i < n; i++) {
        int key = arr[i];
        int j = i - 1;
        while (j >= 0 && arr[j] > key) {
            arr[j + 1] = arr[j];
            j--;
        }
        arr[j + 1] = key;
    }
}

int main() {
    const int N = 10000;
    vector<int> arr(N);

    srand(time(0));
    for (int i = 0; i < N; i++) {
        arr[i] = rand() % 100000; // numeros aleatorios entre 0 y 99,999
    }

    clock_t start = clock();
    insertionSort(arr);
    clock_t end = clock();

    double insertionTime = double(end - start) / CLOCKS_PER_SEC;
    cout << "Tiempo de ejecucion: " << insertionTime << " segundos" << endl;

    return 0;
}
