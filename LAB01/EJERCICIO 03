#include <iostream>
#include <vector>
#include <string>
#include <algorithm> // para sort
using namespace std;

//algoritmo bubbleSort, ordenamiento burbuja
void bubbleSort(vector<string>& lista) {
    int n = lista.size();
    for (int i = 0; i < n - 1; i++) {
        for (int j = 0; j < n - i - 1; j++) {
            if (lista[j] > lista[j + 1]) {
                // intercambiar
                string temp = lista[j];
                lista[j] = lista[j + 1];
                lista[j + 1] = temp;
            }
        }
    }
}
