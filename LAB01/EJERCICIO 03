#include <iostream>
#include <vector>
#include <string>
#include <algorithm> // para sort
using namespace std;

//algoritmo bubbleSort, ordenamiento burbuja
void bubbleSort(vector<string>& lista) {
    int n = lista.size();
    for (int i = 0; i < n - 1; i++) {
        for (int j = 0; j < n - i - 1; j++) {
            if (lista[j] > lista[j + 1]) {
                // intercambiar
                string temp = lista[j];
                lista[j] = lista[j + 1];
                lista[j + 1] = temp;
            }
        }
    }
}
int main() {
    vector<string> nombres1 = {"Juan", "Maria", "Pedro", "Ana", "Luis"};
    vector<string> nombres2 = nombres1; // copia para usar sort

    //lista original
    cout << "Lista original: ";
    for (auto& n : nombres1) cout << n << " ";
    cout << "\n";

    //ordenar con BubbleSort
    bubbleSort(nombres1);
    cout << "Ordenada con BubbleSort: ";
    for (auto& n : nombres1) cout << n << " ";
    cout << "\n";

    //ordenar con sort()
    sort(nombres2.begin(), nombres2.end());
    cout << "Ordenada con sort(): ";
    for (auto& n : nombres2) cout << n << " ";
    cout << "\n";

    return 0;
}
