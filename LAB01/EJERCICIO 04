#include <iostream>
#include <string>
using namespace std;

bool esPalindromo(int numero) {
    string num = to_string(numero); 
    //convierte los valores en una cadena
    int d = num.length(); 
    // cantidad de digitos que tiene el numero
    for (int i = 0; i < d / 2; i++) {
        if (num[i] != num[d - 1 - i]) {
            return false;
        }
    }
    return true;
}

int main() {
    int numero;
    cout << "Ingrese un numero: ";
    cin >> numero;
    if (esPalindromo(numero)) {
        cout << numero << " es palindromo" << endl;
    } else {
        cout << numero << " no es palindromo" << endl;
    }
    return 0;
}